<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuoyi.threebusinesscity">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SplashActivity" />
        <activity android:name=".activity.SupermarketCateringDetailsActivity" />
        <activity android:name=".activity.HotelDetailsActivity" />
        <activity android:name=".activity.GeneralDetailsActivity" />
        <activity android:name=".activity.BindingCellPhoneActivity" />
        <activity android:name=".activity.MyPromoterActivity" />
        <activity android:name=".activity.MyMembersActivity" />
        <activity android:name=".activity.IntegralConsumptionRecordsActivity"></activity>
    </application>

</manifest>